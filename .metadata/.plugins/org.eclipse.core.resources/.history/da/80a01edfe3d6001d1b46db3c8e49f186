package com.java;

public class Date {
		int day, month, year;
		
		Date(){
			day = 01;
			month = 01;
			year = 1970;
		}
		Date(int day){
			this.day = day;
			month = 01;
			year = 1970;
		}
		Date(int day, int month){
			year = 1970;
			this.day = day;
			this.month = month;
		}
		Date(int day, int month, int year){
			this.day = day;
			this.month = month;
			this.year = year;
		}
		
	
		
}
