package com.java;


class Stack {
	int[] arr;
	
	Stack(int size) {
		arr = new int[size];
	}
	
	public void printStack() {
		for(int i=0; i<arr.length; i++) {
			System.err.println(arr[i]);
		}
	}
	
	public void pushToStack(int num) {
		
		int top = 0;
		arr[top] = num;
		top++;
	}
	
	public void popFromStack() {
		
	}
	
	public static void main(String[] args) {}
	
}
