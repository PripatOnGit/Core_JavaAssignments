package com.java;


class Stack {
	int[] arr;
	int top = 0; 
	
	Stack(int size) {
		arr = new int[size];
	}
	
	public void printStack() {
		System.err.println("Elements in array!!");
		for(int i=0; i<arr.length; i++) {			
			System.err.println(arr[i]);
		}
	}
	
	public void pushToStack(int num) {				 
		arr[top] = num;
		top++;
		}
	
	public int pop() {		
		top--;		
		return arr[top];
	}
	
	public static void main(String[] args) {}
	
}
